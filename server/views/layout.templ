package views

templ Layout(titulo string, contenido1 templ.Component, contenido2 templ.Component){
	<!DOCTYPE html>
	<html>
	<head>
		<meta charset="UTF-8">
	    <title> { titulo } </title>
    	<link rel="stylesheet" href="/static/styles.css">
		<script src="https://unpkg.com/htmx.org@1.9.10"></script>
	</head>
	<body>
    	<h1>{ titulo }</h1>
        @contenido1
        @contenido2
	</body>
	</html>
}